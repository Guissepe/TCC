{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plguin:@typescript-eslint/recommended",
        "prettier/@typescript-eslint",
        "plguin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/extension":[
        "error",
        "ignore Packages",
        {
          "ts": "never"
        }
      ],
      "settings": {
        "import/resolver": {
          "typescript": {}
        }
      }
    }
}
